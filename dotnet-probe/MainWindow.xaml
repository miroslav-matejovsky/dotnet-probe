<Window x:Class="dotnet_probe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dotnet_probe"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Azure Monitor" Width="120" Height="40" Margin="0,0,0,10" Click="AzureMonitorButton_Click"/>
            <Button Content="SSO Web" Width="120" Height="40" Margin="0,0,0,10" Click="SsoWebButton_Click"/>
            <Button Content="SSO WPF WAM" Width="120" Height="40" Margin="0,0,0,10" Click="SsoWpfWamButton_Click"/>
            <Button Content="SSO WPF WebView2" Width="120" Height="40" Margin="0,0,0,10" Click="SsoWpfWebView2Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
